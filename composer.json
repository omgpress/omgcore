{
  "name": "oowp/core",
  "version": "1",
  "type": "wordpress-library",
  "description": "",
  "homepage": "https://github.com/oowp/core",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "OOWP Contibutors",
      "email": "",
      "homepage": "https://github.com/oowp"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "config": {
    "platform-check": false,
    "optimize-autoloader": true,
    "autoloader-suffix": "_O0W7_1"
  },
  "autoload": {
    "psr-4": {
      "O0W7_1\\": "inc/"
    }
  },
  "require": {
    "php": "^7.2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3.1",
    "wp-coding-standards/wpcs": "^2.2.1"
  },
  "scripts": {
    "webpack-start": "cd sources && npm run start",
    "webpack-build": "cd sources && npm run build",
    "install-no-dev": "composer install --no-dev",
    "optimize-autoloader": "composer dump-autoload --optimize",
    "lint-php": "phpcs"
  }
}
