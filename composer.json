{
  "name": "dpripa/wp-titan",
  "version": "1.0.1",
  "type": "wordpress-library",
  "description": "One entry point to get all you need for developing a WordPress plugin or a theme.",
  "homepage": "https://github.com/dpripa/wp-titan",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Dmitry Pripa",
      "email": "i@dpripa.com",
      "homepage": "https://dpripa.com"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "config": {
    "platform-check": false,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "WP_Titan_1_0_1\\": "includes/"
    }
  },
  "require": {
    "php": "^7.2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3.1",
    "wp-coding-standards/wpcs": "^2.2.1"
  },
  "scripts": {
    "no-dev": "composer install --no-dev",
    "optimize": "composer dump-autoload --optimize",
    "wpcs": "phpcs",
    "doc": "rm -rf docs/api && phpDocumentor"
  }
}