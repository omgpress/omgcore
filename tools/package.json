{
  "name": "wp-titan-tools",
  "version": "1.0.0",
  "description": "Development tools for WP Titan.",
  "homepage": "https://github.com/dpripa/wp-titan/blob/main/tools",
  "license": "GPL-2.0-or-later",
  "contributors": [
    {
      "name": "Dmitry Pripa",
      "email": "i@dpripa.com",
      "homepage": "https://github.com/dpripa"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "node": "14",
  "scripts": {
    "release": "npm run version && npm zip",
    "zip": "cd .. && composer no-dev && cd tools/scripts && node zip.js && cd ../.. && composer install",
    "version": "cd scripts && node version.js && cd ../.. && composer optimize"
  },
  "dependencies": {
    "fs": "~0.0.1-security",
    "glob": "~7.2.0",
    "archiver": "~5.0.0",
    "log-beautify": "~1.2.0"
  }
}
